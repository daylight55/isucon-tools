Host *
  ServerAliveInterval 300
  AddKeysToAgent yes
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  ForwardAgent yes

Host isu11-1
  HostName xxxx
  #User ubuntu
  User isucon
  IdentityFile ~/.ssh/isucon_id_rsa

Host isu11-2
  HostName xxxx
  #User ubuntu
  User isucon
  IdentityFile ~/.ssh/isucon_id_rsa

Host isu11-bench
  HostName xxxx
  # User ubuntu
  User isucon
  IdentityFile ~/.ssh/isucon_id_rsa
